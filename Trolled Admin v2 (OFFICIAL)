if TextBox.Text == "fix spawn" then 
local UserName  = game:GetService("Players").LocalPlayer.Name
local CE = {}
local C = {
    workspace = game:GetService("Workspace"),
    players = game:GetService("Players")
}
 
local MainUserP, MainUserW = C.players.LocalPlayer, C.workspace:FindFirstChild(UserName)

function savepos()
	savedposition = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
end
function loadpos()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = savedposition
end
function class()
	function class()
		if game:GetService("Players").LocalPlayer.Character:FindFirstChild(1374269) then
			game:GetService("Players").LocalPlayer.Character:FindFirstChild(1374269):Remove()
		end
		if game:GetService("Players").LocalPlayer.Character:FindFirstChild("KittyEars") then
			game:GetService("Players").LocalPlayer.Character:FindFirstChild("KittyEars"):Remove()
		end
	end
end
function CE:FireEvent(ItemName, Object, Property, Value)
    MainUserW:FindFirstChild(ItemName)["ServerControl"]:InvokeServer("SetProperty",{["Value"]=Value,["Property"]=Property,["Object"]=Object})
end
 
workspace.GiveTool:FireServer(1600654, "PompousTheCloud")
MainUserP["Backpack"]:WaitForChild("PompousTheCloud").Parent = MainUserW
savepos()
class()
game.ReplicatedStorage.AvatarEditor.HatHandle:FireServer(1374269)
wait(1)
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"]["Mesh"]:Remove())
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"Shape", "Cylinder")
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"CanCollide",true)
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"Anchored",true)
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"Color", Color3.fromRGB(75, 151, 75))
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"Material","Grass")
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"Size", Vector3.new(4.24627, 121.019, 121.019))
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"Position", Vector3.new(-36.5, 3962.9, 425.942))
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"Orientation", Vector3.new(1.04, -180, -90))
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"Parent", game.Workspace)
class()
loadpos()
end
