RobloxianLife:SetLocalProperty(char:WaitForChild("g0tTr0LLeD is a legend"), "Anchored", true)
RobloxianLife:SetLocalProperty(char["g0tTr0LLeD is a legend"], "Position", Vector3.new(-40, 4000, 425))
RobloxianLife:SetLocalProperty(char["g0tTr0LLeD is a legend"], "CanCollide", true)
RobloxianLife:SetLocalProperty(char["g0tTr0LLeD is a legend"], "Transparency", 0)
RobloxianLife:SetLocalProperty(char["g0tTr0LLeD is a legend"], "Color", Color3.fromRGB(0, 0, 0))
RobloxianLife:SetLocalProperty(char["g0tTr0LLeD is a legend"], "Material", Enum.Material.Neon)
RobloxianLife:SetLocalProperty(char["g0tTr0LLeD is a legend"], "Size", Vector3.new(2000, 1000, 2000))
RobloxianLife:SetLocalProperty(char["g0tTr0LLeD is a legend"], "Rotation", Vector3.new(0, 0, 0))
