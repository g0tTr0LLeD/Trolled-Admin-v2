CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"CanCollide",true)
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"Anchored",true)
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"Color", Color3.fromRGB(255,255,255))
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"Material","ForceField")
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"Transparency","1")
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"Size", Vector3.new(4.24627, 121.019, 121.019))
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"Position", Vector3.new(-36.5, 3962.9, 425.942))
CE:FireEvent("PompousTheCloud",MainUserW["1374269"]["Handle"],"Orientation", Vector3.new(1.04, -180, -90))
