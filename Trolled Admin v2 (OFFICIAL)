loadstring(game:HttpGet("https://raw.githubusercontent.com/g0tTr0LLeD/Trolled-Admin-v2/master/Anticloud%20Checker%20(Testing...Probably%20will%20be%20using%20for%20future)"))()
wait(0.5)
game.ReplicatedStorage.ToggleStrollers:FireServer(false)
game.Workspace.GiveTool:FireServer(7602040, "SuperFlyGoldBoombox")
game.Workspace.GiveTool:FireServer(7602040, "PompousTheCloud")
game.Workspace.GiveTool:FireServer(7602040, "SuperSpeedCoil")
game.StarterGui:SetCore('ChatMakeSystemMessage', {
	Text = "Welcome "..game.Players.LocalPlayer.name.." to Trolled Admin v2";
	Color = Color3.new(255,0,255);
	FontSize = Enum.FontSize.Size18;
})
wait(0.5)
game.StarterGui:SetCore('ChatMakeSystemMessage', {
	Text = "The prefix is .";
	Color = Color3.new(255,0,255);
	FontSize = Enum.FontSize.Size18;
})
wait(0.5)
game:GetService("StarterGui"):SetCore("SendNotification", {
	  Title = "g0tTr0LLeD Admin",
	  Text = "Enjoy the admin you fuckers",
	  Icon = "rbxassetid://940043029"
})

local prefix = "."
local lplayer = game:GetService("Players").LocalPlayer
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextBox = Instance.new("TextBox")
local x = Instance.new("TextLabel")

ScreenGui.Parent = game.CoreGui

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.new(1, 1, 1)
Frame.Position = UDim2.new(0, 17, 0.725, 0)
Frame.Size = UDim2.new(0, 160, 0, 62)
Frame.Style = Enum.FrameStyle.RobloxRound

TextBox.Parent = Frame
TextBox.BackgroundColor3 = Color3.new(1, 1, 1)
TextBox.BackgroundTransparency = 0.80000001192093
TextBox.BorderColor3 = Color3.new(0.968628, 0, 1)
TextBox.BorderSizePixel = 3
TextBox.Size = UDim2.new(0, 144, 0, 45)
TextBox.Font = Enum.Font.Gotham
TextBox.Text = "Type a Command!"
TextBox.TextColor3 = Color3.new(0, 0, 0)
TextBox.TextScaled = true
TextBox.TextSize = 14
TextBox.TextWrapped = true

spawn(function()
    while true do
        for i = 0, 1, 0.010 do
            TextBox.TextColor3 = Color3.fromHSV(i, 1, 1)
            wait()
        end
    end
end)
spawn(function()
    while true do
        for i = 0, 1, 0.010 do
            TextBox.BorderColor3 = Color3.fromHSV(i, 1, 1)
            wait()
        end
    end
end)
game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
	if key == prefix then
		TextBox:CaptureFocus()
		wait()
		if string.sub(TextBox.Text, 1, string.len(prefix)):lower() == tostring(prefix):lower() then
			TextBox.Text = string.sub(TextBox.Text, string.len(prefix) + 1)
		end
	end
end)
TextBox.FocusLost:Connect(function()
TextBox.Text = "T"
wait()
TextBox.Text = "Ty"
wait()
TextBox.Text = "Typ"
wait()
TextBox.Text = "Type"
wait()
TextBox.Text = "Type "
wait()
TextBox.Text = "Type a"
wait()
TextBox.Text = "Type a "
wait()
TextBox.Text = "Type a c"
wait()
TextBox.Text = "Type a Co"
wait()
TextBox.Text = "Type a Com"
wait()
TextBox.Text = "Type a Comm"
wait()
TextBox.Text = "Type a Comma"
wait()
TextBox.Text = "Type a Comman"
wait()
TextBox.Text = "Type a Command"
wait()
TextBox.Text = "Type a Command!"
end)
TextBox.FocusLost:Connect(function()
if TextBox.Text == "cmds" then
	loadstring(game:HttpGet("https://pastebin.com/raw/ftg2xLgV"))()
end
if TextBox.Text == "remove" then
    game.Players.LocalPlayer.PlayerGui.MainGUI:Destroy()
end
if TextBox.Text == "antiaudio" then    
game.Workspace.GiveTool:FireServer(7602040,"BoomboxGearThree")
local ME = game:GetService("Players").LocalPlayer
function FireEvent(ItemName, Object, Property, Value)
  ME.Character:FindFirstChild(ItemName).ServerControl:InvokeServer("SetProperty", {Value = Value, Property = Property, Object = Object}) 
end
ME.Backpack:WaitForChild("BoomboxGearThree").Parent = ME.Character
ME.Backpack:WaitForChild("SuperFlyGoldBoombox").Parent = ME.Character
ME.Backpack:WaitForChild("PompousTheCloud").Parent = ME.Character
FireEvent("PompousTheCloud",ME.Character.SuperFlyGoldBoombox.Client, "Disabled", "true")
FireEvent("PompousTheCloud", ME.Character.SuperFlyGoldBoombox, "Parent", game:GetService("StarterPack"))
wait(1)
FireEvent("PompousTheCloud",ME.Character.BoomboxGearThree.Client, "Disabled", "true")
FireEvent("PompousTheCloud", ME.Character.BoomboxGearThree, "Parent", game:GetService("StarterPack"))
MainUserW:WaitForChild("PompousTheCloud").Parent = MainUserP["Backpack"]
end
if TextBox.Text == "mute" then    
for i,v in next,game:GetDescendants() do
if v:IsA("Sound") then 
v.Playing = false
end
end
end
if TextBox.Text == "pmute" then    
for _, v in pairs(game:GetDescendants()) do
        if v:IsA("Sound") and not v:IsDescendantOf(game.Players.LocalPlayer.Character) then 
            v:Stop()
            v.Volume = 0
        end
    end
end
if TextBox.Text == "lpmute" then    
_G.mute = true

while _G.mute do
    for _, v in pairs(game:GetDescendants()) do
        if v:IsA("Sound") and not v:IsDescendantOf(game.Players.LocalPlayer.Character) then 
            v:Stop()
            v.Volume = 0
        end
    end
wait(0.1)
end
end
if TextBox.Text == "unlpmute" then    
_G.mute = false
end
if TextBox.Text == "trolledvis" then    
local nig = "10000"

local UserName  = game:GetService('Players').LocalPlayer.Name
local Autoride = true

local CE = {}
local Variables = {
	workspace = game:GetService("Workspace"),
	players = game:GetService("Players")
	}
local MainUserP, MainUserW = Variables.players.LocalPlayer, Variables.workspace:FindFirstChild(UserName)

function BreakAnimation()
	for _, x in next, MainUserP.Character.Humanoid:GetPlayingAnimationTracks() do
		if not string.find(x.Animation.AnimationId, "180426354") then
			x:stop()
		end
	end
	wait()
	local DualAnimation = Instance.new("Animation")
	DualAnimation.AnimationId = "rbxassetid://1145142527"
	local DualAnimation_ = MainUserP.Character.Humanoid:LoadAnimation(DualAnimation)
	DualAnimation_:Play()
	DualAnimation_:AdjustSpeed(1)
end

function HideCloud()
	MainUserW.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]="rbxassetid://0",["Property"]="MeshId",["Object"]=MainUserW.PompousTheCloud.Handle.Mesh})
end

local VerifyCloud = function()
	if not MainUserP.Character:FindFirstChild("PompousTheCloud") and not MainUserP.Backpack:FindFirstChild("PompousTheCloud") then
		wait(0.5)
		MainUserP.Backpack:FindFirstChild("PompousTheCloud").Parent = MainUserP.Character
		HideCloud()
		wait()
		BreakAnimation()
		wait(0.5)
		if Autoride == true then
			Variables.workspace[UserName].PompousTheCloud.ServerControl:InvokeServer("Fly", {["Flying"] = true})
			MainUserW.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]="rbxassetid://0",["Property"]="MeshId",["Object"]=MainUserW.PompousTheCloud.EffectCloud.Mesh})
		end
	elseif not MainUserP.Character:FindFirstChild("PompousTheCloud") and MainUserP.Backpack:FindFirstChild("PompousTheCloud") then
		MainUserP.Backpack:FindFirstChild("PompousTheCloud").Parent = MainUserP.Character
		HideCloud()
		wait()
		BreakAnimation()
		wait(0.5)
		if Autoride == true then
			Variables.workspace[UserName].PompousTheCloud.ServerControl:InvokeServer("Fly", {["Flying"] = true})
		end
	elseif MainUserP.Character:FindFirstChild("PompousTheCloud") then
		HideCloud()
		wait()
		BreakAnimation()
		if Autoride == true then
			Variables.workspace[UserName].PompousTheCloud.ServerControl:InvokeServer("Fly", {["Flying"] = false})
		end
	end
end

function CE:FireEvent(ItemName, Object, Property, Value)
	MainUserW:FindFirstChild(ItemName)["ServerControl"]:InvokeServer("SetProperty",{["Value"]=Value,["Property"]=Property,["Object"]=Object})
end

VerifyCloud()
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["LocalScript"],"Disabled",true)
CE:FireEvent("PompousTheCloud",MainUserW["SuperFlyGoldBoombox"]["Handle"],"Transparency","1")
game.ReplicatedStorage.AvatarEditor.HatHandle:FireServer("323418594")wait(1)
CE:FireEvent("PompousTheCloud",MainUserW["323418594"]["Handle"]["Mesh"],"Offset",Vector3.new(0,0,0.37))
CE:FireEvent("PompousTheCloud",MainUserW["323418594"]["Handle"]["Mesh"],"MeshId","rbxassetid://499505913")
--CE:FireEvent("PompousTheCloud",MainUserW["323418594"]["Handle"]["Mesh"],"TextureId","Ball")
CE:FireEvent("PompousTheCloud",MainUserW["323418594"]["Handle"],"Material","ForceField")
spawn(function()
game:service'RunService'.RenderStepped:connect(function()
local asd = math.floor(MainUserW["SuperFlyGoldBoombox"]["Handle"]["BoomboxSound"].PlaybackLoudness)/nig
CE:FireEvent("PompousTheCloud",MainUserW["323418594"]["Handle"]["Mesh"],"Scale",Vector3.new(asd,asd,asd))
end)
end)
pcall(function()
    spawn(function()
        while true do
            for i = 0, 100 do
                CE:FireEvent("PompousTheCloud", MainUserW["323418594"]["Handle"], "Color", Color3.fromHSV(i/100, 1, 1))
                wait()
            end
        end
    end)
end)
end
