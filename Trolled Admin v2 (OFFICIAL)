CE:FireEvent("PompousTheCloud",MainUserW["323418594"]["Handle"],"CFrame", CFrame.new(-36.5, 3962.8999, 425.941986, 3.93390656e-05, -1, 3.57627869e-07, -0.999834538, -3.93390656e-05, -0.0181888342, 0.0181888342, 3.57627869e-07, -0.999834538))
CE:FireEvent("PompousTheCloud",MainUserW["323418594"]["Handle"],"Position", Vector3.new(-36.5, 3963.7, 425.942))
CE:FireEvent("PompousTheCloud",MainUserW["323418594"]["Handle"],"Orientation", Vector3.new(1.04, -180, -90))
CE:FireEvent("PompousTheCloud",MainUserW["323418594"]["Handle"],"Name", "g0tIsTheBest")
CE:FireEvent("PompousTheCloud",MainUserW["323418594"]["g0tIsTheBest"],"Parent", MainUserW)
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["LocalScript"],"Disabled",true)

CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["LocalScript"],"Disabled",true)
CE:FireEvent("PompousTheCloud",MainUserW["SuperFlyGoldBoombox"]["Handle"]["BoomboxSound"],"EmitterSize","99999")
CE:FireEvent("PompousTheCloud",MainUserW["SuperFlyGoldBoombox"]["Handle"]["BoomboxSound"],"MaxDistance","99999")
CE:FireEvent("PompousTheCloud",MainUserW["SuperFlyGoldBoombox"]["Handle"]["BoomboxSound"],"Volume","10")
CE:FireEvent("PompousTheCloud",MainUserW["SuperFlyGoldBoombox"]["Handle"]["BoomboxSound"],"PlaybackLoudness","99999")
CE:FireEvent("PompousTheCloud",MainUserW["SuperFlyGoldBoombox"]["Handle"]["BoomboxSound"],"EmitterSize","99999")
CE:FireEvent("PompousTheCloud",MainUserW["SuperFlyGoldBoombox"]["Handle"]["BoomboxSound"],"MaxDistance","99999")
wait(0.01)
CE:FireEvent("PompousTheCloud",MainUserW["SuperFlyGoldBoombox"]["Handle"]["BoomboxSound"],"Volume","10")
CE:FireEvent("PompousTheCloud",MainUserW["SuperFlyGoldBoombox"]["Handle"]["BoomboxSound"],"PlaybackLoudness","99999")
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["Handle"],"Transparency","1")
spawn(function()
game:service'RunService'.RenderStepped:connect(function()
local asd = math.floor(MainUserW["SuperFlyGoldBoombox"]["Handle"]["BoomboxSound"].PlaybackLoudness)/nig
CE:FireEvent("PompousTheCloud",MainUserW["g0tIsTheBest"],"Size",Vector3.new(2,asd,asd))
end)
end)

