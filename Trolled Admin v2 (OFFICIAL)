if TextBox.Text == "btools" then
local UserName  = game:GetService('Players').LocalPlayer.Name
local Name, ToolTip = "Delete", "Delete anything in the fucking game"
local player, character = game.Players.LocalPlayer, game.Players.LocalPlayer.Character
local P = game.Players
local LP = P.LocalPlayer
--
function fire(Object, Property, Value)
    character:FindFirstChild("PompousTheCloud")["ServerControl"]:InvokeServer("SetProperty",{["Value"]=Value,["Property"]=Property,["Object"]=Object})
end
function RemoveAnim()
    for i,z in next,LP.Character.Humanoid:GetPlayingAnimationTracks() do
        if not string.find(z.Animation.AnimationId,"180426354") then
            z:stop()
        end
    end
end
function DualAnim()
    local DualAnimation = Instance.new("Animation")
    DualAnimation.AnimationId = "rbxassetid://1145142527"
    local DualAnimation_ = game:GetService('Players').LocalPlayer.Character.Humanoid:LoadAnimation(DualAnimation)
    DualAnimation_:Play()
    DualAnimation_:AdjustSpeed(1)
end
function CheckCloud()
if character:FindFirstChild("PompousTheCloud") then
    --
elseif player.Backpack:FindFirstChild("PompousTheCloud") then
    player.Backpack:FindFirstChild("PompousTheCloud").Parent = character
else
game.workspace.Buy:FireServer(0, "PompousTheCloud")
player["Backpack"]:WaitForChild("PompousTheCloud").Parent = character
end
end
function CloudInvis()
fire(character.PompousTheCloud.Handle.Mesh,"MeshId","rbxassetid://1561976844")
end
function Cloud()
CheckCloud()
CloudInvis()
RemoveAnim()
DualAnim()
end
Cloud()

fire(character["PompousTheCloud"],"TextureId","rbxassetid://1574282536")
fire(character["PompousTheCloud"],"ToolTip",ToolTip)
fire(character["PompousTheCloud"]["LocalScript"],"Disabled",true)
fire(character["PompousTheCloud"],"Name",Name)

local Mouse = game:GetService('Players').LocalPlayer:GetMouse()
player:GetMouse().Button1Down:Connect(function()
    if character:FindFirstChild("Delete") then
        local pomp = character:FindFirstChild("Delete")
        pomp.ServerControl:InvokeServer("Fly", {["Flying"] = true})
        wait()
        pomp.ServerControl:InvokeServer("SetProperty",{["Value"]=Mouse.Target,["Property"]="Part0",["Object"]=pomp.EffectCloud.Weld})
        pomp.ServerControl:InvokeServer("SetProperty",{["Value"]=CFrame.new(99999, 99999, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1),["Property"]="CFrame",["Object"]=pomp.EffectCloud})
        wait()
        wait()
        pomp.ServerControl:InvokeServer("Fly", {["Flying"] = false})
    end
end)
end
