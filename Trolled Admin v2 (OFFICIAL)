if TextBox.Text == "blunt" then    
game.workspace.GiveTool:FireServer("7602040", "PompousTheCloud")
        wait()
        local UserName  = game:GetService('Players').LocalPlayer.Name
local Autoride = true

local Icon, Mesh, Texture = 992254977, 4453340376, 4453340516
local Name, ToolTip = "BLUNT", "Blunt"

local CE = {}
local Variables = {
    workspace = game:GetService("Workspace"),
    players = game:GetService("Players")
}

local MainUserP, MainUserW = Variables.players.LocalPlayer, Variables.workspace:FindFirstChild(UserName)

function CE:FireEvent(ItemName, Object, Property, Value)
    MainUserW:FindFirstChild(ItemName)["ServerControl"]:InvokeServer("SetProperty",{["Value"]=Value,["Property"]=Property,["Object"]=Object})
end

Variables.workspace.Buy:FireServer(0, "PompousTheCloud")
MainUserP["Backpack"]:WaitForChild("PompousTheCloud").Parent = MainUserW
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["LocalScript"],"Disabled",true)
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"],"CanBeDropped",true)

CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"],"TextureId","rbxassetid://"..Icon)

CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["Handle"]["Mesh"],"MeshId","rbxassetid://"..Mesh)
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["Handle"]["Mesh"],"TextureId","rbxassetid://"..Texture)
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["Handle"]["Mesh"],"Scale",Vector3.new(.02,.02,.02))
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["Handle"]["Mesh"],"Orientation",Vector3.new(0,0,0))
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["Handle"]["Mesh"],"Position",Vector3.new(14.981,1.759,-11.839))

CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"],"GripForward",Vector3.new(0, 0, -1))
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"],"GripPos",Vector3.new(1.5, -0.75, -0.6))
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"],"GripRight",Vector3.new(1, 0, 0))
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"],"GripUp",Vector3.new(0, 1, 0))
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"],"ToolTip",ToolTip)


CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["Handle"]["Smoke"],"Enabled",true)
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["Handle"]["Smoke"],"Color",Color3.new(50,255,0))
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["Handle"]["Smoke"],"Opacity","0.1")
end
if TextBox.Text == "rboom" then    
game.Workspace.GiveTool:FireServer(7602040, "SuperFlyGoldBoombox")
game.Workspace.GiveTool:FireServer(7602040, "PompousTheCloud")
local ME = game:GetService("Players").LocalPlayer
function FireEvent(ItemName, Object, Property, Value)
  ME.Character:FindFirstChild(ItemName).ServerControl:InvokeServer("SetProperty", {Value = Value, Property = Property, Object = Object}) 
end
ME.Backpack:WaitForChild("SuperFlyGoldBoombox").Parent = ME.Character
ME.Backpack:WaitForChild("PompousTheCloud").Parent = ME.Character
FireEvent("PompousTheCloud", ME.Character.SuperFlyGoldBoombox.Handle.Mesh, "MeshId", "rbxassetid://1323932869")
FireEvent("PompousTheCloud", ME.Character.SuperFlyGoldBoombox.Handle.Mesh, "TextureId", "rbxassetid://1323932922")
FireEvent("PompousTheCloud", ME.Character.SuperFlyGoldBoombox.Handle.Mesh, "Scale", Vector3.new(1, 1, 1))
FireEvent("PompousTheCloud", ME.Character.SuperFlyGoldBoombox.Handle.Mesh, "Offset", Vector3.new(1, 0, .5))
FireEvent("PompousTheCloud", ME.Character.SuperFlyGoldBoombox.Handle.BoomboxSound,"SoundId","144833146")
FireEvent("PompousTheCloud", ME.Character.SuperFlyGoldBoombox.Handle.BoomboxSound,"Volume","10")
FireEvent("PompousTheCloud",ME.Character.SuperFlyGoldBoombox,"CanBeDropped", true)
FireEvent("PompousTheCloud",ME.Character.SuperFlyGoldBoombox,"TextureId","940043029")
FireEvent("PompousTheCloud",ME.Character.SuperFlyGoldBoombox,"ToolTip","g0tTr0LLeD custom boombox")
FireEvent("PompousTheCloud", ME.Character.SuperFlyGoldBoombox.Handle, "Material", Enum.Material.ForceField)
FireEvent("PompousTheCloud", ME.Character.SuperFlyGoldBoombox, "GripRight", Vector3.new(1, 0, 0))
FireEvent("PompousTheCloud",ME.Character.PompousTheCloud.Handle,"Transparency","1")
while true do 
	FireEvent("PompousTheCloud", ME.Character.SuperFlyGoldBoombox.Handle, "BrickColor", BrickColor.new("Really red"))
	wait(.1)
	FireEvent("PompousTheCloud", ME.Character.SuperFlyGoldBoombox.Handle, "BrickColor", BrickColor.new("Deep orange"))
	wait(.1)
	FireEvent("PompousTheCloud", ME.Character.SuperFlyGoldBoombox.Handle, "BrickColor", BrickColor.new("New Yeller"))
	wait(.1)
    FireEvent("PompousTheCloud", ME.Character.SuperFlyGoldBoombox.Handle, "BrickColor", BrickColor.new("Lime green"))
	wait(.1)
	FireEvent("PompousTheCloud", ME.Character.SuperFlyGoldBoombox.Handle, "BrickColor", BrickColor.new("Dark blue"))
	wait(.1)
	FireEvent("PompousTheCloud", ME.Character.SuperFlyGoldBoombox.Handle, "BrickColor", BrickColor.new("Mulberry"))
end
end

game:GetService("UserInputService").JumpRequest:connect(function()
if InfiniteJumpEnabled then
game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
end
end)
	
if TextBox.Text == "infj" then    
	InfiniteJumpEnabled = true
end
if TextBox.Text == "uninfj" then    
	InfiniteJumpEnabled = false
end
if TextBox.Text == "stroller" then    
	game.Workspace.GiveTool:FireServer("7602040", "Stroller")
end
if TextBox.Text == "dagger" then    
game.workspace.GiveTool:FireServer("7602040", "PompousTheCloud")
        wait()
local UserName  = game:GetService('Players').LocalPlayer.Name
local Autoride = true

local Animations = {Shoot = 186934753}
local Icon, Mesh, Texture = 37898424, 867579699, 867579708
local Name, ToolTip = "BOOMER", "Dagger"

local CE = {}
local Variables = {
	workspace = game:GetService("Workspace"),
	players = game:GetService("Players")
}

local MainUserP, MainUserW = Variables.players.LocalPlayer, Variables.workspace:FindFirstChild(UserName)

function CE:FireEvent(ItemName, Object, Property, Value)
	MainUserW:FindFirstChild(ItemName)["ServerControl"]:InvokeServer("SetProperty",{["Value"]=Value,["Property"]=Property,["Object"]=Object})
end

Variables.workspace.Buy:FireServer(0, "PompousTheCloud")
MainUserP["Backpack"]:WaitForChild("PompousTheCloud").Parent = MainUserW

CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"],"TextureId","rbxassetid://"..Icon)

CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["Handle"]["Mesh"],"MeshId","rbxassetid://"..Mesh)
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["Handle"]["Mesh"],"TextureId","rbxassetid://"..Texture)
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["Handle"]["Mesh"],"Scale",Vector3.new(.005,.005,.005))
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["Handle"]["Mesh"],"Orientation",Vector3.new(-0.23,12.94,1.05))
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["Handle"]["Mesh"],"Position",Vector3.new(-111.387,3.568,-35.084))

CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"],"GripForward",Vector3.new(-0.545, -0.089, -0.834))
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"],"GripPos",Vector3.new(0, 0.6, 0))
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"],"GripRight",Vector3.new(1, -0.026, 0))
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"],"GripUp",Vector3.new(-0.027, 1.996, -0.089))

CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"],"ToolTip",ToolTip)
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"],"CanBeDropped",true)
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"]["LocalScript"],"Disabled",true)
CE:FireEvent("PompousTheCloud",MainUserW["PompousTheCloud"],"Name",Name)
wait()

local BOOMER = MainUserW["BOOMER"]

MainUserP:GetMouse().Button1Down:Connect(function()
	if MainUserW:FindFirstChild("BOOMER") then
		local target =  MainUserP:GetMouse().Target.Parent:FindFirstChild("HumanoidRootPart") or MainUserP:GetMouse().Target.Parent.Parent:FindFirstChild("HumanoidRootPart")
		
		Maintarget = target.Parent
				MainUserW.BOOMER.Handle.Wind:Play()
		CE:FireEvent("BOOMER",BOOMER,"GripRight",Vector3.new(0.838, -0.026, -0.545))
		local Animation = Instance.new("Animation")
		Animation.AnimationId = "rbxassetid://186934753"
		local AnimationLoader = MainUserP.Character.Humanoid:LoadAnimation(Animation)
		AnimationLoader:Play()
		AnimationLoader:AdjustSpeed(1)
		wait()
		Variables.workspace.GuiEvent:FireServer(".")
		--wait(0.5)
		
		MainUserW.BOOMER.Handle.Wind.Looped = false
		CE:FireEvent("BOOMER",MainUserW.BOOMER.Handle.Wind,"SoundId","rbxassetid://935843979")
		CE:FireEvent("BOOMER",MainUserW.BOOMER.Handle.Wind,"Volume",10)
		CE:FireEvent("BOOMER",MainUserW.BOOMER.Handle.Wind,"MaxDistance",9e9)
		CE:FireEvent("BOOMER",MainUserW.BOOMER.Handle.Wind,"EmitterSize",9e9)
		CE:FireEvent("BOOMER",MainUserW.BOOMER.Handle.Wind,"Playing",true)
		CE:FireEvent("BOOMER",MainUserW.BOOMER.Handle.Wind,"PlaybackSpeed",1)
		CE:FireEvent("BOOMER",MainUserW["."]:WaitForChild("Head"),"Parent",Maintarget)
		CE:FireEvent("BOOMER",MainUserW["BOOMER"],"GripRight",Vector3.new(1, -0.026, 0))
		wait(1.500)
		MainUserW.BOOMER.Handle.Wind:Stop()
	end
end)
end
