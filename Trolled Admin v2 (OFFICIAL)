if string.sub(TextBox.Text,1,8) == "control " then
    for f,g in pairs(GetPlayer(string.sub(TextBox.Text,9)))do
    game.Workspace.GiveTool:FireServer(a, "PompousTheCloud")
    local UserName = game:GetService("Players").LocalPlayer.Name
    local Variables = {
    workspace = game:GetService("Workspace"),
    players = game:GetService("Players")
}

    local MainUserP, MainUserW = Variables.players.LocalPlayer, Variables.workspace:FindFirstChild(UserName)
    if MainUserP["Backpack"]:FindFirstChild("PompousTheCloud") then
        MainUserP["Backpack"]:WaitForChild("PompousTheCloud").Parent=MainUserW
    end
    function Txl(ItemName, Object, Property, Value)
        MainUserW:FindFirstChild(ItemName)["ServerControl"]:InvokeServer("SetProperty",{["Value"]=Value,["Property"]=Property,["Object"]=Object})
    end
    for L,n in pairs(MainUserW:GetDescendants())do 
        if n:IsA("BasePart")or n:IsA ("Accessory")then
        Txl("PompousTheCloud",n,"Transparency","1")
    end 
    end
    game.ReplicatedStorage.AvatarEditor.HatHandle:FireServer(14129625)
    Txl("PompousTheCloud",MainUserW["Head"]["face"],"Texture","rbxassetid://0")
    Txl("PompousTheCloud",MainUserW["14129625"]["Handle"],"Transparency","1")
    Txl("PompousTheCloud",MainUserW["14129625"]["Handle"]["AccessoryWeld"],"Part1",game.Players[g.Name].Character.Head)
    Txl("PompousTheCloud",MainUserW["14129625"]["Handle"]["AccessoryWeld"],"Part1",nil)
    Txl("PompousTheCloud",MainUserW["Head"]["NametagTemplate"]["TagText"],"Text","")
    wait(0.5)
end
end
