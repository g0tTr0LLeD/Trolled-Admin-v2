  for i,v in pairs (game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
            v:Stop()
        end

local tool = game.Players.LocalPlayer.Character:WaitForChild("PompousTheCloud")
	tool.ServerControl:InvokeServer("SetProperty",{["Value"]="",["Property"]="MeshId",["Object"]=tool.Handle.Mesh})



for i,tool in pairs(game.Players.LocalPlayer.Character:GetChildren()) do 
if tool.Name == "PompousTheCloud" then 
	for i,localsript in pairs(tool:GetChildren()) do
		if localsript:IsA("LocalScript") then
localsript:Destroy()
		end
end
end
end    
wait(.2)
  for i,v in pairs (game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()) do
            v:Stop()
        end
wait(0.5)

local animation = Instance.new("Animation", tool)
		    animation.AnimationId = "rbxassetid://29517689"
	   local drinker = animation
	  local enabled = false
	  local con = nil
	  local humanoid = game.Players.LocalPlayer.Character.Humanoid
	   local tool = game.Players.LocalPlayer.Character.PompousTheCloud
	  tool.Parent = game.Players.LocalPlayer.Backpack
	  tool.Parent = game.Players.LocalPlayer.Character
	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]="rbxassetid://4885306511",["Property"]="MeshId",["Object"]=tool.Handle.Mesh})
	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]="",["Property"]="TextureId",["Object"]=tool.Handle.Mesh})
	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=Color3.fromRGB(50, 50, 50),["Property"]="Color",["Object"]=tool.Handle})
	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=Vector3.new(1,1,1),["Property"]="Scale",["Object"]=tool.Handle.Mesh})
	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=Vector3.new(0, 1, 0.3),["Property"]="Offset",["Object"]=tool.Handle.Mesh})
	    		 	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=0,["Property"]="Size",["Object"]=tool.Handle.Smoke})
	  	   	  	    		 	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=2,["Property"]="RiseVelocity",["Object"]=tool.Handle.Smoke})
	  	    		 	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=0,["Property"]="Opacity",["Object"]=tool.Handle.Smoke})
	  	    		 	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=true,["Property"]="Enabled",["Object"]=tool.Handle.Smoke})
	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=CFrame.new(0.25, 0.699999988, -0.400000006, 0.99999994, 0, 0, 0, 0.707106769, -0.707106769, -0, 0.707106769, 0.707106769),["Property"]="Grip",["Object"]=tool})
function onEquipped(mouse)
    	humanoid = tool.Parent:FindFirstChild("Humanoid")
	if humanoid ~= nil then
		drinker = humanoid:LoadAnimation(animation)
	end
	con = mouse.Button1Down:connect(function() onButton1Down(mouse) end)
end

function onButton1Down(mouse)

	if enabled then
		return
	end
	
	enabled = true 

	
	drinker:Play() 
	wait(1)
		 	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=0,["Property"]="Opacity",["Object"]=tool.Handle.Smoke})
						 	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=0.1,["Property"]="Opacity",["Object"]=tool.Handle.Smoke})
						 	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=0.2,["Property"]="Opacity",["Object"]=tool.Handle.Smoke})
						 	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=0.3,["Property"]="Opacity",["Object"]=tool.Handle.Smoke})

						 	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=0.4,["Property"]="Opacity",["Object"]=tool.Handle.Smoke})
game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=0.5,["Property"]="Opacity",["Object"]=tool.Handle.Smoke})

	enabled = false
	wait(1.7)
						 	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=0.4,["Property"]="Opacity",["Object"]=tool.Handle.Smoke})
						 	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=0.3,["Property"]="Opacity",["Object"]=tool.Handle.Smoke})
						 	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=0.2,["Property"]="Opacity",["Object"]=tool.Handle.Smoke})
						 	    	game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=0.1,["Property"]="Opacity",["Object"]=tool.Handle.Smoke})
game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=0,["Property"]="Opacity",["Object"]=tool.Handle.Smoke})

end	


spawn(function()
    while true do
        for i = 0, 1, 0.010 do
            game.Players.LocalPlayer.Character.PompousTheCloud.ServerControl:InvokeServer("SetProperty",{["Value"]=Color3.fromHSV(i, 1, 1),["Property"]="Color",["Object"]=tool.Handle})
            wait()
        end
    end
end)

tool.Equipped:connect(onEquipped)
end
end)