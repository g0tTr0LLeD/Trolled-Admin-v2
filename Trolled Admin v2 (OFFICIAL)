VerifyCloud()

for a,b in pairs (MainUserW:GetChildren()) do
    warn(b)
       if b:IsA('Accessory') and  b.DataCost == 4 then
           print(b)
           b:Destroy()
       end
end

for i,v in next, MainUserW:GetChildren() do
        if v:IsA('Accessory') and  v.DataCost == 4 then
            print(b)
            v:Destroy()
        elseif v:IsA('Accessory') and v:GetChildren("Handle") then
            CE:FireEvent("PompousTheCloud",v["Handle"], "Material", "Neon")
            print(v)
        end
    
        for x,y in pairs(v:GetDescendants()) do
            warn(y)
            if y:IsA("Mesh") or y:IsA("SpecialMesh") then
                warn(y)
                CE:FireEvent("PompousTheCloud",y, "TextureId", "")
            end
        end

        pcall(function()
            spawn(function()
                while true do
                    for i = 0, 100 do
                        CE:FireEvent("PompousTheCloud",v["Handle"], "Color", Color3.fromHSV(i/100, 1, 1))
                        wait()
                    end
                end
            end)
        end)
end
